<form action="/parks" method="POST">
 <div class="form-group">
  <label for="name"> Name of the Park </label>
  <input type="text" class="form-control" id="name" placeholder="Enter the name of the park">
  </div>
  <div class="form-group">
  <label for="state"> State </label>
  <input type="text" class="form-control" id="state" placeholder="Which state does it belong to?">
  </div>
  <div class="form-group">
  <label for="country"> Country </label>
  <input type="text" class="form-control" id="country" placeholder="Which country does it belong to?">
  </div>
  <div class="form-group">
  <label for="rating"> Rating </label>
  <input type="text" class="form-control" id="rating" placeholder="What's your rating for the park?">
  </div>
  <div class="form-group">
  <label for="summary"> Summary </label>
  <input type="text" class="form-control" id="summary" placeholder="What's it about?">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>