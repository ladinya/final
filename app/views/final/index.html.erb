<h1> National Parks</h1>

<div class="row">
  <% @parks.each do |park| %>
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
       <%= link_to image_tag(park.image), controller: "final" %>
      <div class="caption">
        <h3><%= link_to park.name, controller: "final" %></h3>
        <p><%= park.summary %></p>
      </div>
    </div>
  </div>
  <% end %>
 </div>
